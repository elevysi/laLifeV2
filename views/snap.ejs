<div class="cbp-l-inline">
    <div class="row">
        <img src="<%= snap.originalPath %>" alt="Dashboard" class="cbp-l-project-img">
    </div>
    <hr>
    <div class="row text-center">
        <div class="col-sm-6">
            <div class="cbp-l-inline-title"><%= snap.name %></div>
            <div class="cbp-l-inline-subtitle"><%= moment(snap.created).format('dddd MMMM Do YYYY') %></div>
            <a href="/view/<%= snap.id %>" class="cbp-l-inline-view">VIEW SNAP</a>
        </div>

        <div class="col-sm-6 cbp-l-inline-desc">
             <%= snap.description %>
        </div>

           
    </div>        
    </div>
</div>